{% extends 'base.html.twig' %}

{% block title %}Cat{% endblock %}

{% block body %}
    <h1>Cat</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>NumICAD</th>
                <td>{{ cat.numICAD }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ cat.name }}</td>
            </tr>
            <tr>
                <th>Sex</th>
                <td>{{ cat.sex }}</td>
            </tr>
            <tr>
                <th>HasDateBirth</th>
                <td>{{ cat.hasDateBirth ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>DateBirth</th>
                <td>{{ cat.dateBirth ? cat.dateBirth|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>DescShort</th>
                <td>{{ cat.descShort }}</td>
            </tr>
            <tr>
                <th>DescLong</th>
                <td>{{ cat.descLong }}</td>
            </tr>
            <tr>
                <th>Id</th>
                <td>{{ cat.id }}</td>
            </tr>
            <tr>
                <th>Race</th>
                <td>{{ cat.race }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_cat_index') }}">back to list</a>

    <a href="{{ path('app_cat_edit', {'id': cat.id}) }}">edit</a>

    {{ include('cat/_delete_form.html.twig') }}
{% endblock %}
